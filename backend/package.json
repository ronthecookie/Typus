{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate save --experimental && prisma migrate up --experimental",
    "emails:compile": "mjml src/emails/submission.mjml -o src/views/emails/submission.ejs"
  },
  "dependencies": {
    "@prisma/client": "2.0.0-beta.2",
    "@tsed/ajv": "5.49.1",
    "@tsed/common": "5.49.0",
    "@tsed/core": "5.49.0",
    "@tsed/di": "5.49.0",
    "ajv": "6.12.2",
    "argon2": "0.26.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.2.0",
    "connect-redis": "4.0.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "ejs": "3.1.2",
    "express": "4.17.1",
    "express-fileupload": "1.1.7-alpha.3",
    "express-session": "1.17.1",
    "ioredis": "4.16.3",
    "liquid": "4.0.1",
    "mailgun-js": "0.22.0",
    "method-override": "3.0.0",
    "mjml": "4.6.2",
    "node-fetch": "2.6.0",
    "ts-httpexceptions": "4.1.0"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-preview025",
    "@types/argon2": "0.15.0",
    "@types/compression": "1.7.0",
    "@types/connect-redis": "0.0.13",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/ejs": "3.0.3",
    "@types/express": "4.17.6",
    "@types/express-fileupload": "1.1.3",
    "@types/express-session": "1.17.0",
    "@types/ioredis": "4.14.9",
    "@types/mailgun-js": "0.22.4",
    "@types/method-override": "0.0.31",
    "@types/node": "13.13.4",
    "@types/node-fetch": "2.5.7",
    "concurrently": "5.2.0",
    "nodemon": "2.0.3",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  }
